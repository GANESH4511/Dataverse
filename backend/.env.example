# CloudFront Configuration
CLOUDFRONT_DOMAIN="https://your-cloudfront-domain.cloudfront.net"

# Server Configuration
PORT=3000

# Solana Configuration (Devnet)
SOLANA_RPC_URL="https://api.devnet.solana.com"
